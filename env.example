# AI Job Processing Platform Environment Variables
# Copy this file to .env and fill in your API keys

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/ai_job_platform

# AI Provider API Keys
GROK_API_KEY=your_grok_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
DASHSCOPE_API_KEY=your_dashscope_api_key_here
ZAI_API_KEY=your_zai_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Default AI Agent (grok, gemini, deepseek, openai, qwen, zai, claude)
DEFAULT_AI_AGENT=openai

# Processing Configuration
MAX_WORKERS=8

# Google Drive Configuration (Optional)
GDRIVE_FOLDER_ID=your_google_drive_folder_id
GDRIVE_SERVICE_ACCOUNT=credentials/service_account.json

# Model Overrides (Optional)
# GROK_MODEL=grok-beta
# GEMINI_MODEL=gemini-2.5-pro
# DEEPSEEK_MODEL=deepseek-chat
# OPENAI_MODEL=gpt-5-mini
# QWEN_MODEL=qwen2.5-72b
# ZAI_MODEL=glm-4-32b-0414-128k
# CLAUDE_MODEL=claude-opus-4.1