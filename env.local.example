# AI Job Processing Platform - Local Development Environment Variables
# Copy this file to .env.local for local development

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/ai_job_platform

# AI Provider API Keys (Optional - add your keys here)
GROK_API_KEY=
GEMINI_API_KEY=
DEEPSEEK_API_KEY=
OPENAI_API_KEY=
DASHSCOPE_API_KEY=
ZAI_API_KEY=

# Default AI Agent (grok, gemini, deepseek, openai, qwen, zai)
DEFAULT_AI_AGENT=grok

# Processing Configuration
MAX_WORKERS=8

# Google Drive Configuration (Optional)
GDRIVE_FOLDER_ID=
GDRIVE_SERVICE_ACCOUNT=credentials/service_account.json